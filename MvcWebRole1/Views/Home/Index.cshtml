@model DataStoreLib.Models.MovieEntity

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MovieLayout.cshtml";
}
<script type="text/javascript">
    $(document).ready(function () {
        LoadCurrentMovies();
    });
</script>

<div class="col-sm-8" style="padding:0">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Current Movies</h3>
        </div>
        <div class="panel-body">
            <div style="float: left; width: 103%; min-height: 10px; position: relative; top: 74px; margin: 0 -13px; cursor:pointer">
                <div style="float:left;width:10%; min-height:10px;" title="Previous">
                    <img src="~/Images/pervious.png" alt="pervios" />
                </div>
                <div style="float:right; min-height:10px;" title="Next">
                    <img src="~/Images/next.png" alt="pervios" />
                </div>
            </div>
            <div id="currentmovie" style="float: left; width: 96%; margin: 0 2%;">

            </div>
        </div>
    </div>
</div>